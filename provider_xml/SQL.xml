<?xml version="1.0"?>
<SQL-List>
    <MODULE>
        <SQL id="Data.Character_sets.Query" type="mysql">
            <TEXT>
            SELECT  @CHARACTER_SET_NAME,  @DEFAULT_COLLATE_NAME,  @DESCRIPTION,  @MAXLEN FROM Character_sets limit @Count
            </TEXT>
        </SQL>
        <SQL id="Data.Character_sets.Query2" type="mysql">
            <TEXT>
            SELECT  CHARACTER_SET_NAME,  DEFAULT_COLLATE_NAME,  DESCRIPTION,  MAXLEN FROM Character_sets limit 8
            </TEXT>
        </SQL>
        <SQL id="Data.Character_sets.Query_Single" type="mysql">
            <TEXT>
            SELECT  @CHARACTER_SET_NAME,  @DEFAULT_COLLATE_NAME,  @DESCRIPTION,  @MAXLEN FROM Character_sets limit 1
            </TEXT>
        </SQL>
        <SQL id="Data.Character_sets.Query_Single_null" type="mysql">
            <TEXT>
            SELECT  @CHARACTER_SET_NAME,  @DEFAULT_COLLATE_NAME,  @DESCRIPTION,  @MAXLEN FROM Character_sets where 1!=1
            </TEXT>
        </SQL>
    </MODULE>
</SQL-List>
